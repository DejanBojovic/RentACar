
@model List<RentACar.Models.MembershipType>

@{
    ViewBag.Title = "Random";
}

<h1>Membership Types</h1>

<h3>List of Membership Types</h3>
@foreach (var mt in Model)
{
    <div style="padding: 10px 15px; background-color: #f4f4f4; border-radius: 8px; margin-bottom: 10px; display: flex; justify-content: space-between; align-items: center;">
        <p style="margin-bottom: 0">Monts: @mt.DurationInMonths</p>
        

    </div>
}



